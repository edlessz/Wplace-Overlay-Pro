{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": ["ES2021", "DOM", "WebWorker"],
		"strict": false,
		"skipLibCheck": true,
		"allowJs": true,
		"checkJs": false,
		"resolveJsonModule": true,
		"verbatimModuleSyntax": true,
		"types": ["tampermonkey"],
		"noEmit": true
	},
	"include": ["src"]
}
